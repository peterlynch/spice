      <implementation>org.sonatype.jsecurity.realms.FakeRealm</implementation>
      <description></description>
      <isolated-realm>false</isolated-realm>
    </component>
    <component>
      <role>org.sonatype.jsecurity.locators.RealmLocator</role>
      <implementation>org.sonatype.jsecurity.locators.PropertyFileRealmLocator</implementation>
      <description></description>
      <isolated-realm>false</isolated-realm>
      <configuration>
        <property-file>${realm-locator-property-file}</property-file>
      </configuration>
    </component>
    <component>
      <role>org.sonatype.jsecurity.locators.RememberMeLocator</role>
      <implementation>org.sonatype.jsecurity.locators.DefaultRememberMeLocator</implementation>
      <description></description>
      <isolated-realm>false</isolated-realm>
    </component>
  </components>
</component-set>
